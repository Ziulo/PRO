Proceso sin_titulo
	//Programa que calcule la media de tres números pedidos al usuario.
	Definir a, b, c Como Real;;
	Escribir " Introduce un numero: ";
	Leer a;
	Escribir " Introduce otro numero: ";
	Leer b;
	Escribir " Introduce otro numero: ";
	Leer c;
	Escribir "La media de ", a ,", ", b ," y ", c ," es: ", (a+b+c)/3; 

FinProceso
