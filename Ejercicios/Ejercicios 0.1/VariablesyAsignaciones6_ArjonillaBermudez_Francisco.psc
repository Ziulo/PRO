Proceso sin_titulo
	//Programa para resolver ecuaciones de segundo grado

	
	Definir a, b, c Como real;
	Escribir " En una ecuacion de segundo grado completa del tipo ax^2 + bx + c = 0 con dos soluciones reales";
	Escribir "Introduce a";
	Leer a;
	Escribir "Introduce b";
	Leer b;
	Escribir "Introduce c";
	Leer c;
	Escribir " La primera solucion sera x=", (-b+raiz(b^2-4*a*c))/(2*a) ;
    Escribir " La segunda solucion sera x=", (-b-raiz(b^2-4*a*c))/(2*a) ;
FinProceso
